pipeline{
    agent any
    stages
    {
        stage('scm checkout')
        {steps{git branch: 'master', url: 'https://github.com/Devopstech321/hello-world.git'}}
        stage('create artifact')
        {
            steps{withMaven(jdk: 'JAVA-HOME', maven: 'MAVN-HOME') {
    sh 'mvn clean package'
}}
        }
        }
}
