pipeline
{
agent any
stages
{
    stage ('copy code from github')
    {
        steps {git branch: 'master', url: 'https://github.com/Devopstech321/hello-world.git'}
    }
    stage ('creating artifacts')
    {
        steps {withMaven(jdk: 'Java-home', maven: 'MVN-Home') {
    sh 'mvn package'
    }}
    }
